{
  "openapi": "3.1.0",
  "info": {
    "title": "My API",
    "version": "1.0.0",
    "description": "API documentation"
  },
  "components": {
    "schemas": {
      "UserCreatedWebhookSchema": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "number",
            "minimum": 1
          },
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          }
        },
        "required": [
          "id_user",
          "username"
        ],
        "additionalProperties": false
      },
      "UserProfileSchema": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "number"
          },
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          },
          "avatar": {
            "type": "string"
          },
          "status": {
            "type": "number"
          },
          "last_connection": {
            "type": "string"
          }
        },
        "required": [
          "id_user",
          "username",
          "avatar",
          "status",
          "last_connection"
        ]
      },
      "UsernameSchema": {
        "type": "string",
        "minLength": 4,
        "maxLength": 16,
        "pattern": "^[A-Za-z0-9_-]{4,16}$"
      },
      "UserProfileSchemaOutput": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "number"
          },
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          },
          "avatar": {
            "type": "string"
          },
          "status": {
            "type": "number"
          },
          "last_connection": {
            "type": "string"
          }
        },
        "required": [
          "id_user",
          "username",
          "avatar",
          "status",
          "last_connection"
        ],
        "additionalProperties": false
      },
      "PublicUserListSchema": {
        "type": "object",
        "properties": {
          "users": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserCreatedWebhookSchema"
            }
          }
        },
        "required": [
          "users"
        ],
        "additionalProperties": false
      },
      "PublicUserSchema": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "number",
            "minimum": 1
          },
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          }
        },
        "required": [
          "id_user",
          "username"
        ],
        "additionalProperties": false
      },
      "UserAccountSchema": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "integer",
            "exclusiveMinimum": 0,
            "maximum": 9007199254740991
          },
          "avatar": {
            "type": "string"
          },
          "status": {
            "type": "integer",
            "minimum": 0,
            "maximum": 1
          },
          "last_connection": {
            "type": "string"
          }
        },
        "required": [
          "id_user",
          "avatar",
          "status",
          "last_connection"
        ],
        "additionalProperties": false
      },
      "UserIdSchema": {
        "type": "object",
        "properties": {
          "id_user": {
            "type": "integer",
            "exclusiveMinimum": 0,
            "maximum": 9007199254740991
          }
        },
        "required": [
          "id_user"
        ],
        "additionalProperties": false
      },
      "UserProfileListSchema": {
        "type": "object",
        "properties": {
          "users": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserProfileSchemaOutput"
            }
          }
        },
        "required": [
          "users"
        ],
        "additionalProperties": false
      },
      "ErrorResponseSchema": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "error": {
            "type": "string"
          }
        },
        "required": [
          "error"
        ],
        "additionalProperties": false
      },
      "SuccessResponseSchema": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "const": true
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "success"
        ],
        "additionalProperties": false
      },
      "LoginSchema": {
        "type": "object",
        "properties": {
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          },
          "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 128
          }
        },
        "required": [
          "username",
          "password"
        ],
        "additionalProperties": false
      },
      "RegisterSchema": {
        "type": "object",
        "properties": {
          "username": {
            "$ref": "#/components/schemas/UsernameSchema"
          },
          "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 128
          }
        },
        "required": [
          "username",
          "password"
        ],
        "additionalProperties": false
      }
    }
  }
}